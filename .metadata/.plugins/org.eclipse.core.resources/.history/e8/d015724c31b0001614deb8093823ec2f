package classes;

public class Song {
	private String songURI;
	private long duration;
	
	public Song(String songURI, long duration){
		this.songURI = songURI;
		this.duration = duration;
	}

	public String getSongURI() {
		return songURI;
	}

	public long getSongDuration() {
		return duration;
	}

}
